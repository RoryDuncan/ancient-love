{
    "$schema": "https://raw.githubusercontent.com/TypeScriptToLua/TypeScriptToLua/master/tsconfig-schema.json",
    "compilerOptions": {
      "target": "ESNext",
      "lib": ["ESNext"],
      "moduleResolution": "Node",
      "types": [],
      "strict": true
    },
    "files": ["main.ts"],
    "tstl": {
        "noImplicitGlobalVariables": true,
        "buildMode": "default",
        "noImplicitSelf": true,
        "luaTarget": "5.4"
    }
  }